{
  "version": 2,
  "name": "uncover",
  "env": {
    "APP_URL": "@uncover-app-url",
    "APOLLO_ENDPOINT": "@uncover-apollo-endpoint",
    "APOLLO_ENDPOINT_WSS": "@uncover-apollo-endpoint-wss",
    "AUTH0_CLIENT_ID": "@uncover-auth0-client-id",
    "AUTH0_CLIENT_DOMAIN": "@uncover-auth0-client-domain",
    "CLOUDINARY_NAME": "@uncover-cloudinary-name",
    "CLOUDINARY_PRESET": "@uncover-cloudinary-preset"
  },
  "build": {
    "env": {
      "APP_URL": "@uncover-app-url",
      "APOLLO_ENDPOINT": "@uncover-apollo-endpoint",
      "APOLLO_ENDPOINT_WSS": "@uncover-apollo-endpoint-wss",
      "AUTH0_CLIENT_ID": "@uncover-auth0-client-id",
      "AUTH0_CLIENT_DOMAIN": "@uncover-auth0-client-domain",
      "CLOUDINARY_NAME": "@uncover-cloudinary-name",
      "CLOUDINARY_PRESET": "@uncover-cloudinary-preset"
    }
  },
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/now-builder",
      "config": {}
    }
  ]
}
